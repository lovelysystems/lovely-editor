{
  "name": "OyezEditor",
  "version": "0.1.0",
  "description": "Oyez Editor",
  "private": true,
  "main": "app/index.js",
  "author": "Stefan Natter <natter@lovelysystems.com>",
  "license": "MIT",
  "engines": {
    "node": ">=6.11",
    "npm": ">=3.10"
  },
  "scripts": {
    "bundle": "cd example && webpack",
    "lint": "npm run lint:js && npm run lint:styles",
    "lint:js": "eslint src/**/*.js --fix",
    "lint:styles": "stylelint \"./src/**/*.scss\" --fix",
    "prestart": "npm run test:node",
    "start": "cd example && webpack-dev-server",
    "pretest": "npm run lint",
    "test": "_mocha --require babel-core/register --require src/tests/helpers/browser.js --require ignore-styles --colors \"./src/**/*.spec.js\"",
    "test:node": "./node_modules/check-node-version/bin.js --node '>='$(cat .nvmrc) --npm '>= 3.10.0'"
  },
  "pre-commit": [
    "lint:js",
    "lint:styles",
    "test"
  ],
  "keywords": [
    "lovelysystems",
    "ls-editor",
    "editor"
  ],
  "peerDependencies": {
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  },
  "dependencies": {
    "lodash": "4.17.4",
    "prop-types": "15.6.0",
    "react": "16.0.0",
    "react-bem-helper": "1.4.1",
    "react-dom": "16.0.0"
  },
  "devDependencies": {
    "autoprefixer": "7.1.5",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "chai": "4.1.2",
    "check-node-version": "2.1.0",
    "css-loader": "0.28.7",
    "enzyme": "3.1.0",
    "enzyme-adapter-react-16": "1.0.2",
    "eslint": "4.9.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-config-prettier": "2.6.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-prettier": "2.3.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.1",
    "ignore-styles": "5.0.1",
    "jsdom": "11.3.0",
    "mocha": "4.0.1",
    "node-sass": "4.5.3",
    "nodemon": "1.12.1",
    "postcss-loader": "2.0.8",
    "pre-commit": "1.2.2",
    "prettier": "1.7.4",
    "react-test-renderer": "16.0.0",
    "sass-loader": "6.0.6",
    "stylelint": "8.2.0",
    "stylelint-config-sass-guidelines": "3.1.0",
    "webpack": "3.6.0",
    "webpack-dev-server": "2.9.2"
  }
}
